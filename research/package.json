{
  "name": "serverless-pm-research",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "latexmk -pdf -pvc Research.tex",
    "build": "latexmk -pdf Research.tex",
    "build:watch": "latexmk -pdf -pvc Research.tex",
    "format": "latexindent Research.tex",
    "clean": "latexmk -c",
    "clean:all": "latexmk -C",
    "preview": "open Research.pdf || xdg-open Research.pdf"
  }
}
