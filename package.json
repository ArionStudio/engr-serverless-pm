{
  "name": "serverless-pm",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "install:all": "pnpm install",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint",
    "ext:dev": "pnpm --filter serverless-pm-extension run dev",
    "ext:build": "pnpm --filter serverless-pm-extension run build",
    "ext:test": "pnpm --filter serverless-pm-extension run test",
    "ext:lint": "pnpm --filter serverless-pm-extension run lint",
    "ext:format": "pnpm --filter serverless-pm-extension run format",
    "res:dev": "pnpm --filter serverless-pm-research run dev",
    "res:build": "pnpm --filter serverless-pm-research run build",
    "res:format": "pnpm --filter serverless-pm-research run format",
    "res:clean": "pnpm --filter serverless-pm-research run clean"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1"
  }
}
